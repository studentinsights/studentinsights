{
  "name": "studentinsights",
  "version": "0.1.0",
  "engines": {
    "node": "^16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/studentinsights/studentinsights.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"bundle exec rails s\" \"yarn dev\"",
    "test": "yarn jest --watch",
    "dev": "NODE_ENV=development webpack --config ui/config/webpack.dev.js --watch",
    "storybook": "start-storybook -p 6006 --config-dir ui/config/.storybook --quiet --ci",
    "build": "NODE_ENV=production webpack --config ui/config/webpack.prod.js",
    "test-all-cli": "yarn yarn-checks-cli && yarn lint-cli && yarn jest && yarn audits-cli && yarn storybook-cli",
    "jest": "TZ='America/New_York' jest --config jest.json",
    "lint": "yarn lint-cli; exit 0",
    "lint-cli": "yarn eslint --ext js -c .eslintrc app/assets/javascripts ui",
    "lockfile-lint-cli": "lockfile-lint -t yarn --path yarn.lock --allowed-hosts registry.yarnpkg.com --validate-https",
    "audits-cli": "yarn audit && yarn auditjs-cli",
    "auditjs-cli": "yarn auditjs --package yarn.lock",
    "storybook-cli": "yarn storybook --smoke-test",
    "yarn-checks-cli": "yarn check --integrity"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "chroma-js": "^2.1.0",
    "csv-stringify": "^5.3.6",
    "d3": "3.5.12",
    "es5-shim": "^4.5.15",
    "highcharts": "^9.2.2",
    "hosted-git-info": "^2.8.9",
    "jquery": "^3.5.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.6",
    "moment": "2.29.2",
    "npm": "^7.24.2",
    "object-hash": "^2.0.1",
    "percentile": "^1.2.0",
    "query-string": "^5",
    "react": "~16.8.6",
    "react-app-polyfill": "^1.0.6",
    "react-autocomplete": "^1.8.1",
    "react-beautiful-dnd": "^11.0.2",
    "react-debounce-input": "^3.2.0",
    "react-dom": "~16.8.6",
    "react-modal": "^3.11.1",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.3.0",
    "react-virtualized": "9.20",
    "rollbar": "^2.14.0",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^3.0.0",
    "wordcloud": "^1.1.0",
    "y18n": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-actions": "6.1.9",
    "@storybook/react": "^6.4.0-beta.26",
    "auditjs": "4.0.36",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^8.0.1",
    "concurrently": "^6.3.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-react": "^7.18.3",
    "fetch-mock": "^8.3.2",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-fetch-mock": "^3.0.3",
    "lockfile-lint": "^3.0.5",
    "react-test-renderer": "~16.8.6",
    "script-loader": "^0.7.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^5.1.14",
    "webpack": "^5",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "css-what": ">=5.0.1",
    "glob-parent": "^5.1.2",
    "node-fetch": "^2.6.7",
    "path-parse": "^1.0.7",
    "prismjs": "^1.24.0",
    "@storybook/react/**/browserslist": "^4.16.5",
    "@storybook/react/**/postcss": "^8.2.13",
    "@storybook/react/**/lodash-es": "^4.17.15",
    "@storybook/react/**/immer": "^9.0.6",
    "@storybook/react/**/nth-check": "^2.0.1",
    "@storybook/react/**/set-value": "^4.0.1",
    "@storybook/**/trim": "^0.0.3",
    "tar": "^6.1.11"
  }
